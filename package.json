{
  "name": "ticketing_system",
  "version": "0.0.1",
  "private": true,
  "description": "A ticketing system built on the Internet Computer",
  "author": "Muhammad Akmal Hisyam",
  "license": "MIT",
  "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background",
    "replica_stop": "dfx stop",
    "canister_deploy_local": "dfx deploy ticketing_system",
    "canister_deploy_mainnet": "dfx deploy --network ic ticketing_system",
    "canister_uninstall": "dfx canister uninstall-code ticketing_system",
    "canister_call": "dfx canister call ticketing_system"
  },
  "dependencies": {
    "azle": "^0.18.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akmalhisyammm/ticketing-system-icp"
  },
  "bugs": {
    "url": "https://github.com/akmalhisyammm/ticketing-system-icp/issues"
  }
}
